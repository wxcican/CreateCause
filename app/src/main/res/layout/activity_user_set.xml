<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:orientation="vertical"    android:layout_width="match_parent"    android:background="@color/colorBackround"    android:layout_height="match_parent">    <View      style="@style/ViewTitle"/>    <include layout="@layout/base_title"/>    <LinearLayout        android:id="@+id/ll_set_1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:background="@color/colorWhite">        <TextView            android:padding="10dp"            android:text="关于创业宗"            android:textColor="@color/colorHandler"            android:textSize="16sp"            android:layout_width="0dp"            android:layout_weight="1"            android:layout_height="wrap_content" />        <ImageView            android:layout_marginRight="10dp"            android:layout_gravity="center_vertical"            android:background="@drawable/icon_more"            android:layout_width="wrap_content"            android:layout_height="wrap_content" />    </LinearLayout>    <View        style="@style/ViewDivHorizontal"/>    <LinearLayout        android:id="@+id/ll_set_2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:background="@color/colorWhite">        <TextView            android:padding="10dp"            android:text="意见反馈"            android:textColor="@color/colorHandler"            android:textSize="16sp"            android:layout_width="0dp"            android:layout_weight="1"            android:layout_height="wrap_content" />        <ImageView            android:layout_marginRight="10dp"            android:layout_gravity="center_vertical"            android:background="@drawable/icon_more"            android:layout_width="wrap_content"            android:layout_height="wrap_content" />    </LinearLayout>    <View        style="@style/ViewDivHorizontal"/>    <Space        android:layout_width="match_parent"        android:layout_height="10dp" />    <View        style="@style/ViewDivHorizontal"/>    <LinearLayout        android:id="@+id/ll_set_3"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:background="@color/colorWhite">        <TextView            android:padding="10dp"            android:text="隐私协议"            android:textColor="@color/colorHandler"            android:textSize="16sp"            android:layout_width="0dp"            android:layout_weight="1"            android:layout_height="wrap_content" />        <ImageView            android:layout_marginRight="10dp"            android:layout_gravity="center_vertical"            android:background="@drawable/icon_more"            android:layout_width="wrap_content"            android:layout_height="wrap_content" />    </LinearLayout>    <View        style="@style/ViewDivHorizontal"/>    <LinearLayout        android:id="@+id/ll_set_4"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:background="@color/colorWhite">        <TextView            android:padding="10dp"            android:text="版本更新"            android:textColor="@color/colorHandler"            android:textSize="16sp"            android:layout_width="0dp"            android:layout_weight="1"            android:layout_height="wrap_content" />        <TextView            android:id="@+id/tv_set_1"            android:textSize="14sp"            android:textColor="@color/colorBody"            android:text="当前版本: 1.0.0"            android:layout_width="wrap_content"            android:layout_height="wrap_content" />        <ImageView            android:layout_marginRight="10dp"            android:layout_gravity="center_vertical"            android:background="@drawable/icon_more"            android:layout_width="wrap_content"            android:layout_height="wrap_content" />    </LinearLayout>    <View        style="@style/ViewDivHorizontal"/>    <Space        android:layout_width="match_parent"        android:layout_height="0dp"        android:layout_weight="1"/>    <TextView        style="@style/ViewButton"        android:id="@+id/bt_set_1"        android:layout_marginBottom="40dp"        android:text="退出登录" /></LinearLayout>