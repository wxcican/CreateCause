<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    android:layout_width="match_parent"    android:layout_height="wrap_content"    android:background="@color/colorWhite"    android:orientation="vertical">    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:orientation="horizontal"        android:padding="10dp">        <ImageView            android:id="@+id/iv_item_user_payment_type_1"            android:layout_width="40dp"            android:layout_height="40dp" />        <TextView            android:ellipsize="end"            android:lines="1"            android:id="@+id/tv_item_user_payment_type"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_marginLeft="10dp"            android:layout_weight="1"            android:gravity="center_vertical"            android:text="支付宝"            android:textColor="@color/colorInCome"            android:textSize="15sp" />        <ImageView            android:id="@+id/iv_item_user_payment_type_2"            android:layout_width="20dp"            android:layout_height="20dp"            android:layout_gravity="center_vertical"            android:src="#FFFFFF" />    </LinearLayout></LinearLayout>