<?xml version="1.0" encoding="utf-8"?><LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/colorBackround"    android:orientation="vertical"    tools:context="com.mzth.createcause.view.fragment.user.eaaaymessage.ReleaseActivity">    <View style="@style/ViewTitle" />    <include layout="@layout/base_title" />    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="标题"            android:textColor="#424242"            android:textSize="15sp" />        <EditText            android:id="@+id/et_user_release_1"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_weight="4"            android:background="@null"            android:hint="请输入“一网资讯”标题"            android:paddingRight="10dp"            android:textSize="14sp" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="标题"            android:textColor="#424242"            android:textSize="15sp" />        <EditText            android:id="@+id/et_user_release_4"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_weight="4"            android:background="@null"            android:hint="请输入“山西信息港”标题"            android:paddingRight="10dp"            android:textSize="14sp" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="关键字"            android:textColor="#424242"            android:textSize="15sp" />        <EditText            android:id="@+id/et_user_release_2"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_weight="4"            android:background="@null"            android:ellipsize="start"            android:hint="用逗号隔开"            android:textSize="14sp" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="简介"            android:textColor="#424242"            android:textSize="15sp" />        <EditText            android:id="@+id/et_user_release_3"            android:layout_width="0dp"            android:layout_height="match_parent"            android:layout_weight="4"            android:background="@null"            android:ellipsize="start"            android:hint="请输入简介"            android:textSize="14sp" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:id="@+id/ll_user_release_1"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="分类"            android:textColor="#424242"            android:textSize="15sp" />        <TextView            android:id="@+id/tv_user_release_1"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:gravity="center_vertical"            android:text="请选择"            android:textColor="@color/colorBody"            android:textSize="14sp" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center_vertical"            android:layout_marginRight="10dp"            android:background="@drawable/icon_more" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="vertical">        <TextView            android:layout_width="match_parent"            android:layout_height="wrap_content"            android:padding="10dp"            android:text="封面"            android:textColor="#424242"            android:textSize="15sp" />        <RelativeLayout            android:id="@+id/rv_user_release_1"            android:layout_width="240dp"            android:layout_height="165dp"            android:layout_gravity="center"            android:layout_marginBottom="20dp"            android:layout_marginTop="10dp"            android:background="@color/colorBackround"            android:gravity="center">            <LinearLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:gravity="center"                android:orientation="vertical">                <ImageView                    android:id="@+id/iv_back"                    android:layout_width="71dp"                    android:layout_height="65dp"                    android:background="@drawable/article_image_gray" />                <TextView                    android:layout_width="wrap_content"                    android:layout_height="wrap_content"                    android:layout_marginTop="20dp"                    android:text="建议上传尺寸480*330px"                    android:textColor="#cdcdcd"                    android:textSize="14sp" />            </LinearLayout>            <ImageView                android:id="@+id/iv_user_release_1"                android:layout_width="match_parent"                android:layout_height="match_parent"                android:scaleType="fitXY" />        </RelativeLayout>    </LinearLayout>    <View style="@style/ViewDivHorizontal" />    <LinearLayout        android:id="@+id/ll_user_release_2"        android:layout_width="match_parent"        android:layout_height="wrap_content"        android:background="@color/colorWhite"        android:orientation="horizontal">        <TextView            android:id="@+id/tv_user_release_2"            android:layout_width="0dp"            android:layout_height="wrap_content"            android:layout_weight="1"            android:paddingBottom="10dp"            android:paddingLeft="10dp"            android:paddingTop="10dp"            android:text="编辑文章"            android:textColor="#424242"            android:textSize="15sp" />        <TextView            android:id="@+id/tv_user_release_3"            android:layout_width="wrap_content"            android:layout_height="match_parent"            android:gravity="center_vertical"            android:text="未编辑"            android:textColor="@color/colorBody"            android:textSize="14sp" />        <ImageView            android:layout_width="wrap_content"            android:layout_height="wrap_content"            android:layout_gravity="center_vertical"            android:layout_marginRight="10dp"            android:background="@drawable/icon_more" />    </LinearLayout>    <View style="@style/ViewDivHorizontal" /></LinearLayout>